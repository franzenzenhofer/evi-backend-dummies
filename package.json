{
  "name": "evi-backend-dummies",
  "version": "1.0.0",
  "description": "Interactive HTML prototypes for EVI.gv.at workshop",
  "scripts": {
    "build": "node build.js",
    "lint": "eslint components/**/*.html components/**/*.js build.js --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "npm run lint && npm run typecheck && npm run build",
    "watch": "nodemon --watch components --exec npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/franzenzenhofer/evi-backend-dummies.git"
  },
  "author": "Franz Enzenhofer",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-html": "^7.0.0",
    "typescript": "^5.0.0"
  }
}
